{
    "name": "rschoonheim/filament-page",
    "description": "Page builder for FilamentPHP",
    "type": "library",
    "authors": [
        {
            "name": "Romano Schoonheim",
            "email": "romano@romanoschoonheim.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "Rschoonheim\\FilamentPage\\": "src/"
        }
    },
    "require": {
        "symfony/yaml": "^6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Rschoonheim\\FilamentPage\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^7.14"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rschoonheim\\FilamentPage\\Laravel\\Providers\\FilamentPageBuilderServiceProvider"
            ]
        }
    }
}
